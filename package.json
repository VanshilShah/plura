{
  "name": "plura",
  "version": "1.0.0",
  "description": "A todolist tool",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "node-sass --include-path scss public/stylesheets/stylesheet.scss public/stylesheet.css",
    "watch-css": "nodemon -e scss -x npm run build-css",
    "build-webpack-release": "npx webpack --config webpack.config.js --mode=production",
    "watch-webpack": "npx webpack --config webpack.config.js --watch",
    "build-jsx-release": "npx babel components --out-dir public/react-components --presets react-app/prod",
    "watch-jsx": "npx babel --watch components --out-dir public/react-components --presets react-app/dev",
    "build-start": "npm run watch-css & npm run watch-jsx & npm run watch-webpack & gin run main.go",
    "start": "go run main.go",
    "build-release": "npm install && npm run build-css && npm run build-jsx-release && npm run build-webpack-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanshilShah/plura.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VanshilShah/plura/issues"
  },
  "homepage": "https://github.com/VanshilShah/plura#readme",
  "dependencies": {
    "@date-io/moment": "^1.3.7",
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/pickers": "^3.1.2",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "firebase": "^6.2.4",
    "moment": "^2.24.0",
    "notistack": "^0.8.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "moment-locales-webpack-plugin": "^1.0.7",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4"
  }
}
